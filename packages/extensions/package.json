    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.280",
    "typescript": "^5.6.3",
    "wxt": "^0.19.13"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "key-display": "workspace:*"
  }
}
